    "jsdom": "^27.4.0",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "vitest-browser-react": "^2.0.5",
    "web-vitals": "^5.1.0"
