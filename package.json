    "@tanstack/react-router-devtools": "^1.157.16",
    "@tanstack/router-plugin": "^1.157.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/user-event": "^14.6.1",
    "lucide-react": "^0.562.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "vite-tsconfig-paths": "^6.0.5"
